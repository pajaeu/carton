{
    "name": "pajaeu/carton",
    "description": "The missing cart package for Laravel.",
    "homepage": "https://github.com/pajaeu/carton",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Skrbel",
            "email": "pavel.skrbel@codetiv.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Carton\\Carton\\": "src/",
            "Carton\\Carton\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carton\\Carton\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carton\\Carton\\CartonServiceProvider"
            ],
            "aliases": {
                "Carton": "Carton\\Carton\\Facades\\Carton"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
